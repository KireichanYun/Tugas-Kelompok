// Jawaban nomor 1

#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

int main() {
    string infix, str = "";
    int len;
    
    getline(cin, infix);
    
    len = infix.size();
    
    for(int i=0; i<len; i++) {
        if(isalnum(infix[i])) {
            str += infix[i];
            if(!(isalnum(infix[i+1])))
                str += ' ';
        }
        else if(infix[i]=='-') {
            if(isalnum(infix[i+1])) {
                str += infix[i];
                if(i!=0)
                    str += ' ';
            }
            else if(infix[i+1]=='(') {
                if(isalnum(infix[i-1])) {
                    str += infix[i];
                    str += ' ';
                }
                else {
                    str += "-1 * ";
                }
            }
            else {
                if(infix[i+1]!=' ')
                    str += "-1";
                else
                    str += infix[i];
                str += ' ';
            }
        }
        else {
            if(infix[i]!=' ') {
                str += infix[i];
                str += ' ';
            }
        }
    }
    
    cout << str;
}
